{
  "name": "vuemu",
  "version": "0.2.4",
  "private": true,
  "dependencies": {
    "@material-ui/core": "^4.11.0",
    "@material-ui/icons": "^4.2.1",
    "@material-ui/lab": "^4.0.0-alpha.56",
    "@sentry/browser": "^5.10.2",
    "@sentry/cli": "^1.49.0",
    "@types/camelcase-keys-deep": "^0.1.0",
    "@types/jest": "24.0.18",
    "@types/node": "12.7.4",
    "@types/react": "16.9.2",
    "@types/react-dom": "16.9.0",
    "@types/react-redux": "^7.1.2",
    "@types/react-router-dom": "^5.1.0",
    "bundlesize": "^0.18.0",
    "camelcase-keys-deep": "^0.1.0",
    "connected-react-router": "^6.6.0",
    "date-fns": "^2.14.0",
    "decamelize-keys-deep": "^0.1.1",
    "dotenv": "^8.2.0",
    "eslint-config-prettier": "^6.2.0",
    "prettier": "^1.18.2",
    "react": "^16.9.0",
    "react-dom": "^16.9.0",
    "react-query": "^2.25.2",
    "react-redux": "^7.1.1",
    "react-router-dom": "^5.1.2",
    "react-scripts": "^3.3.0",
    "redux": "^4.0.4",
    "redux-saga": "^1.1.1",
    "redux-sentry-middleware": "^0.1.3",
    "source-map-explorer": "^2.0.1",
    "typescript": "^3.9.6"
  },
  "scripts": {
    "start": "react-scripts start",
    "build": "react-scripts build",
    "test": "react-scripts test",
    "test-bundlesize": "bundlesize",
    "sentry": "node sentry-release.js",
    "deploy": "npm run build && npm run sentry",
    "lint": "eslint \"src/**/*.{js,jsx,tsx}\" --quiet",
    "format": "prettier --write \"src/**/*.{js,jsx,tsx}\"",
    "analyze": "source-map-explorer 'build/static/js/*.js'",
    "eject": "react-scripts eject"
  },
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "bundlesize": [
    {
      "path": "./build/static/js/@(browse|main|playlist).*.chunk.js",
      "maxSize": "10 kB"
    },
    {
      "path": "./build/static/js/4.*.chunk.js",
      "maxSize": "130 kB"
    }
  ],
  "eslintConfig": {
    "extends": [
      "react-app",
      "prettier",
      "prettier/react"
    ]
  },
  "prettier": {}
}
